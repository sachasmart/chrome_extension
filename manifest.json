

{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Frugal",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "icons" :{
        "16": "./icon.png",
        "32": "./icon.png",
        "48": "./icon.png",
        "128": "./icon.png"
    },
    "permissions": [
        "tabs",
        "https://*/*",
        "https://*/"

    ], 
    "host_permissions": [
    "https://*/*"
  ]
}